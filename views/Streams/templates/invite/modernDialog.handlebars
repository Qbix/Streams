    <div class="Q_buttons Streams_invite_social_buttons">
        <button class="Q_button Streams_invite_button_sms" data-sendBy="sms">
            <div class="Streams_invite_social_icons"></div>
            <div class="Streams_invite_social_labels">{{text.bySms}}</div>
        </button>
        <button class="Q_button Streams_invite_button_email" data-sendBy="email">
            <div class="Streams_invite_social_icons"></div>
            <div class="Streams_invite_social_labels">{{text.byEmail}}</div>
        </button>
        <button class="Q_button Streams_invite_button_telegram" data-sendBy="telegram">
            <div class="Streams_invite_social_icons"></div>
            <div class="Streams_invite_social_labels">{{text.byTelegram}}</div>
        </button>
        <div class="Streams_invite_button_break"></div>
        <button class="Q_button Streams_invite_button_whatsapp" data-sendBy="whatsapp">
            <div class="Streams_invite_social_icons"></div>
            <div class="Streams_invite_social_labels">{{text.byWhatsapp}}</div>
        </button>
        <button class="Q_button Streams_invite_button_facebook" data-sendBy="facebook">
            <div class="Streams_invite_social_icons"></div>
            <div class="Streams_invite_social_labels">{{text.byFacebook}}</div>
        </button>
        <button class="Q_button Streams_invite_button_twitter" data-sendBy="twitter">
            <div class="Streams_invite_social_icons"></div>
            <div class="Streams_invite_social_labels">{{text.byTwitter}}</div>
        </button>
    </div>
    {{#if showLabelsButtons}}
    <div class="Streams_invite_label_buttons">
        {{#if showGrantRolesButton}}
            <button class="Streams_invite_button_add-roles">
                {{text.addRoles}}
                {{#if communityRolesNum}}<span>{{ communityRolesNum }}</span>{{/if}}
            </button>
        {{/if}}
        {{#if showGrantRelationshipsButtonButton}}
        <button class="Streams_invite_button_add_rels">
            {{text.addPersonalRoles}}
            {{#if myLabelsNum}}<span>{{ myLabelsNum }}</span>{{/if}}
        </button>
        {{/if}}
    </div>
    {{/if}}
</div>